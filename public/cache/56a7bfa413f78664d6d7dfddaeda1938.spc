    ";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";s:5:"child";a:2:{s:0:"";a:6:{s:6:"author";a:1:{i:0;a:5:{s:4:"data";s:12:"Lily Kelting";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:11:"description";a:1:{i:0;a:5:{s:4:"data";s:856:"Jazzfest at the Haus der Berliner Festspiele presents an international festival that both charts important landmarks in jazz history, like the 50th anniversary of the Globe Unity Orchestra, and celebrates up-and-coming artists. The festival runs through Sunday, November 6th at Schaperstrasse 24. From November 3rd - 5th, science and art meet at the STATE festival. This year's theme is the STATE of emotion, and brings together neuroscientists, artists, historians, and engineers. The conference and many of the art exhibitions take place in the Kühlhaus at Luckenwalder Strasse 3. On Saturday November 5th and Sunday November 6th at 8 pm, artist Katia Guedes presents "ein-gebrannt", a staged concert of five pieces for soprano and ensemble. The theme of the evening: What it means to be a woman. It's at the Acker Stadt Palast at Ackerstrasse 169/170. ";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:5:"title";a:1:{i:0;a:5:{s:4:"data";s:28:"Three Things To Do This Week";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:4:"link";a:1:{i:0;a:5:{s:4:"data";s:52:"http://www.nprberlin.de/post/three-things-do-week-43";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:4:"guid";a:1:{i:0;a:5:{s:4:"data";s:32:"72454 as http://www.nprberlin.de";s:7:"attribs";a:1:{s:0:"";a:1:{s:11:"isPermaLink";s:5:"false";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:7:"pubDate";a:1:{i:0;a:5:{s:4:"data";s:31:"Mon, 31 Oct 2016 15:20:10 +0000";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explici